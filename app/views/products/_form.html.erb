<%= form_with model: product do |form| %>
    <div>
      <%= form.label :name %>
      <%= form.text_field :name %>
    </div>
    <div>
      <%= form.label :color %>
      <%= form.text_field :color %>
    </div>
    

 
     <%= form.date_select :birth_date %>
  


    <div>
      <%= form.submit %>
    </div>
  <% end %>
  